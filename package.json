  "scripts": {
    "postinstall": "npm run webpack:vendor",
    "start": "webpack-dev-server --config=./.webpack/webpack.config.js --cert=./.ssl/localhost.crt --hot --https --key=./.ssl/localhost.key",
    "test": "npm run test:lint && npm run test:vulnerabilities",
    "test:lint": "npm run test:lint:styles && npm run test:lint:scripts",
    "test:lint:scripts": "eslint --ext .js --ext .jsx .",
    "test:lint:styles": "stylelint \"src/**/*.scss\"",
    "test:vulnerabilities": "npm audit",
    "version": "echo $npm_package_version",
    "webpack:profile": "webpack --config=./.webpack/webpack.config.js --json --profile > webpack.profile.json",
    "webpack:vendor": "webpack --config=./.webpack/webpack.config.dll.js"
  },
